{
    "name": "qlik-tools",
    "displayName": "Qlik Tools",
    "description": "Qlik Tools",
    "publisher": "InfoVizion",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.33.0"
    },
    "repository": {"type": "git", "url": "https://github.com/vadimtsushko/qlikcode.git"},
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "qlik",
            "aliases": ["qlik", "qlik"],
            "extensions": [".qvs"],
            "configuration": "./language-configuration.json"
        },
        {
            "id": "qlik-expressions",
            "aliases": ["qlik-expressions", "qlik-expressions"],
            "extensions": [".qlikview-vars"],
            "configuration": "./language-configuration.json"
        }
        
        ],
        "grammars": [{
            "language": "qlik",
            "scopeName": "source.qvs",
            "path": "./syntaxes/qlik.tmLanguage.json"
        },
        {
            "language": "qlik-expressions",
            "scopeName": "source.qlik-expressions",
            "path": "./syntaxes/expression.tmLanguage.json"
        }
    ]
    }
}